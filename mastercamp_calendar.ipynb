   "source": [
    "import pandas as pd \n",
    "import requests \n",
    "import icalendar \n",
    "from datetime import datetime as dt\n",
    "import pytz\n",
    "from dateutil import tz"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "data = requests.get(\"https://www.myefrei.fr/api/public/student/planning/AcSWV5Tr0fs9ulk5_8-XKQ\").text\n",
    "# parse the iCalendar data into a Calendar object\n",
    "cal = icalendar.Calendar.from_ical(data)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "# iterate over the calendar events and extract the relevant information\n",
    "events = []\n",
    "for event in cal.walk('VEVENT'):\n",
    "    events.append({\n",
    "        'dtstart': event.get('DTSTART').dt,\n",
    "        'dtend': event.get('DTEND').dt,\n",
    "        'summary': str(event.get('SUMMARY')),\n",
    "        'description': str(event.get('DESCRIPTION')),\n",
    "        'location': str(event.get('LOCATION'))\n",
    "    })\n",
    "\n",
    "# create a data frame from the extracted information\n",
    "df = pd.DataFrame(events)"
   ]
  },
